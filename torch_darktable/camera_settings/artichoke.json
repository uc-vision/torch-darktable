{
  "type": "camera_settings",
  "name": "artichoke",
  "image_size": [
    4096,
    3000
  ],
  "padding": 0,
  "bayer_pattern": "RGGB",
  "packed_format": "Packed12",
  "white_balance": null,
  "image_processing": {
    "type": "image_processing_settings",
    "tone_gamma": 1.5,
    "tone_intensity": 2.0,
    "light_adapt": 0.8,
    "vibrance": 0.5,
    "moving_average": 0.02,
    "debayer": "rcd",
    "ppg_median_threshold": 0.0,
    "postprocess": true,
    "green_eq_threshold": 0.04,
    "color_smoothing_passes": 3,
    "enable_bilateral": true,
    "bilateral": 0.4,
    "bil_sigma_spatial": 2.0,
    "bil_sigma_luminance": 0.2,
    "enable_denoise": true,
    "denoise": 0.075,
    "tone_mapping": "adaptive_aces",
    "resize_width": 0
  },
  "transform": "rotate_270"
}